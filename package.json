{
  "private": false,
  "name": "lycg",
  "version": "1.0.4-beat",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d dist",
    "build": "gulp",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "release": "npm run build && npm publish",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Liyi-up/lycg",
    "branch": "main"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "ahooks": "^2.10.12",
    "antd-mobile": "^5.0.0-rc.7",
    "caniuse-lite": "^1.0.30001292",
    "compressorjs": "^1.1.1",
    "lodash": "^4.17.21",
    "macy": "^2.5.1",
    "qrcode.react": "^1.0.1",
    "react": "^16.12.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.5",
    "@babel/plugin-transform-modules-commonjs": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-react": "^7.16.5",
    "@babel/preset-typescript": "^7.16.5",
    "@types/lodash": "^4.14.178",
    "@umijs/test": "^3.0.5",
    "babel-loader": "^8.2.3",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-transform-async-to-promises": "^0.8.17",
    "css-loader": "^6.5.1",
    "del": "^6.0.0",
    "dumi": "^1.1.0",
    "father-build": "^1.19.1",
    "gh-pages": "^3.2.3",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-less": "^5.0.0",
    "gulp-postcss": "^9.0.1",
    "gulp-rename": "^2.0.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "less": "^4.1.2",
    "lint-staged": "^10.0.7",
    "path": "^0.12.7",
    "prettier": "^1.19.1",
    "style-loader": "^3.3.1",
    "through2": "^4.0.2",
    "ts-node": "^10.4.0",
    "webpack": "^5.65.0",
    "webpack-stream": "^7.0.0",
    "yorkie": "^2.0.0"
  }
